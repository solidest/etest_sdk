topology topo_rt {
	mapping: {
		etest: [udp1_, udp2_, dev1_, dev2_],
	}
	linking: {
		_ : [udp1_.conn1, udp2_.conn1], 
		_ : [udp1_.conn2, udp2_.conn2, udp2_.conn3], 
		_ : [udp1_.conn3, udp2_.conn4], 
		_ : [dev1_.conn1, dev2_.conn1], 
	}
	binding: {
		udp1_.conn1: 'auto',
		udp1_.conn2: 'auto',
		udp1_.conn3: 'auto',
		udp2_.conn1: 'auto',
		udp2_.conn2: 'auto',
		udp2_.conn3: 'auto',
		udp2_.conn4: 'auto',
		dev1_.conn1: 'COM1',
		dev2_.conn1: 'COM2',
	}
}