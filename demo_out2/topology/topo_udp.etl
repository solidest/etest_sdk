topology topo_udp {
	mapping: {
		etest: [udp1, udp2],
	}
	linking: {
		_ : [udp1.conn1, udp2.conn1], 
		_ : [udp1.conn2, udp2.conn2, udp2.conn3], 
		_ : [udp1.conn3, udp2.conn4], 
	}
	binding: {
		udp1.conn1: 'auto',
		udp1.conn2: 'auto',
		udp1.conn3: 'auto',
		udp2.conn1: 'auto',
		udp2.conn2: 'auto',
		udp2.conn3: 'auto',
		udp2.conn4: 'auto',
	}
}