bitlr protocol prot_arrays {
	segment head {
		parser: 'uint64',
		autovalue: 1,
	}
	segments array {
		segment number {
			parser: 'uint8',
			autovalue: 1,
		}
		segments list {
			segment seg_1 {
				parser: 'uint16',
				autovalue: 1,
			}
			segment seg_2 {
				parser: 'string',
				autovalue: '12',
				endswith: '#3x@#@##$%',
			}
		}
	}
}