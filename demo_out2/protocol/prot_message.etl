bitlr protocol prot_message {
	segment seg_message {
		parser: 'uint8',
		autovalue: 0,
	}
	segments seg_messages {
		segment msg_string {
			parser: 'string',
			autovalue: '123',
			endwith: '~',
		}
		segment msg[2] {
			parser: 'string',
			autovalue: ["123","000"],
			length: 3,
		}
		segment uint {
			parser: 'int64',
			autovalue: 0,
		}
	}
}