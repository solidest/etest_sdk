//仿真设备真值测试协议
protocol truvalue {
        
        segment seg_device_name {parser: 'int32'} //代表设备名称
        segment seg_start {parser:'int32'} //启动测试执行
        segment seg_all[8] {parser:'int32'}
        segment seg_end {parser:'int32'} //停止测试执行
        segment seg_true {parser:'int64'} 
        segment seg_init {parser: 'float'} //各个设备恢复初始状态


}


