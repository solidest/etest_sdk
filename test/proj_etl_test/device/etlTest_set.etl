
// 硬件接口
device dev11 {
    udp srv_1 { ip: '192.168.253.1', port: 8081, ttl: 20 }
    tcp_client clt_2 { ip: '127.0.0.1' }
    tcp_server srv_2 { ip: '127.0.0.1', port: 3000 }

    udp c1 { ip: '192.168.253.150', port: 8001 }
    udp c2 { ip: '127.0.0.1', port: 8889 }
    udp udp_name {ip: '0.0.0.0', port: 8888}

    serial_ttl s1 { baudrate: 9600}
    serial_232 s2 { baudrate: 9600 }
    serial_422 s3 { baudrate: 9600 }
    serial_485 s4 { baudrate: 9600 }
 
    di di1 { minv: 3, maxv: 5 }
    do do1 { minv: 3, maxv: 5 }


    da da1 { ratio: 8, minv: 0, maxv: 24 }
    ad ad1 { ratio: 16, minv: 0, maxv: 12 }
}


//etest控制的设备
device dev12{

    udp udp_name {ip: '127.0.0.1',port:8090}
    udp s1 { ip: '0.0.0.0',port:5000}
    udp s2 { ip: '0.0.0.0',port:7000 }
    udp s3 { ip: '127.0.0.1' ,port:8004}
    udp s4 { ip: '127.0.0.1' ,port:8005}
 

}


//拓扑结构
topology topo11 {


    linking: {
        total: [dev12.s1, dev12.s2],   //总线
        link: [ dev12.udp_name,dev11.srv_1], //点到点连接
        link_: [dev12.s3,dev11.c2]
    }

    //设备映射
    mapping: {
        uut: [dev11],   //被测硬件单元
        etest: [dev12], //软设备
    }

    //接口绑定
    binding: {
        dev12.s1: 'auto',
        dev12.s2: 'auto',
        dev12.s3: 'auto',
        dev12.s4: 'auto',
        dev12.udp_name: 'auto',
        
    }
}


//仿真设备1
device dev_a {

    udp conn { ip: '192.168.253.150', port: 8001}
}


//仿真设备2
